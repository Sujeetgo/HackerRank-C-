#include<bits/stdc++.h>
using namespace std;

int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    vector<int>ans;
    for(int i=n-1; i>=0; i--){
        ans.push_back(arr[i]);
    }
    for(auto &val:ans){
        cout<<val<<" ";
    }
    
}
